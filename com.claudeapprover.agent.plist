<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.claudeapprover.agent</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/yoshidaanyuu/bin/ClaudeApprover</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>ThrottleInterval</key>
    <integer>5</integer>
    <key>StandardOutPath</key>
    <string>/tmp/ClaudeApprover.stdout.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/ClaudeApprover.stderr.log</string>
</dict>
</plist>
